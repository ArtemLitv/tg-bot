{
  "name": "tg-bot",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "build:admin-server": "cd admin/server && tsc",
    "build:admin-client": "cd admin/client && react-scripts build",
    "build:admin": "npm run build:admin-server && npm run build:admin-client",
    "build:all": "npm run build && npm run build:admin",
    "start": "node dist/src/index.js",
    "start:admin-server": "node admin/server/dist/index.js",
    "start:admin-client": "cd admin/client && react-scripts start",
    "dev": "tsc && NODE_ENV=development node dist/src/index.js",
    "dev:admin-server": "npx ts-node-dev --respawn admin/server/src/index.ts",
    "dev:admin-client": "cd admin/client && react-scripts start"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "node-telegram-bot-api": "^0.64.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-editor-ajrm": "^2.5.14",
    "react-router-dom": "^6.14.2",
    "react-scripts": "5.0.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "reactflow": "^11.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.0",
    "@types/node-telegram-bot-api": "^0.64.8",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-json-editor-ajrm": "^2.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "private": true,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
